{
  "name": "@seihon/sectionize",
  "description": "All-in-one responsive image processing library with webpage loader, babel macro and React component",
  "version": "0.0.9",
  "main": "lib/index.js",
  "author": "Billy Kwok <xobkwok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "seihon",
    "mdx",
    "webpack",
    "remark"
  ],
  "files": [
    "lib",
    "types"
  ],
  "types": "types/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/billykwok/seihon/packages/sectionize",
  "bugs": {
    "url": "https://github.com/billykwok/seihon/issues"
  },
  "homepage": "https://github.com/billykwok/seihon",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && babel src --root-mode=upward --out-dir lib --extensions .ts,.tsx,.mjs,.js,.jsx",
    "eslint": "eslint ./src/**",
    "dtslint": "dtslint --onlyTestTsNext --localTs ../../node_modules/typescript/lib types",
    "lint": "yarn eslint && yarn dtslint",
    "test": "jest"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "unist-util-find-after": "^2.0.4",
    "unist-util-visit-parents": "^3.0.1"
  }
}
