{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": { "@seihon/sectionize": ["."] }
  }
}
